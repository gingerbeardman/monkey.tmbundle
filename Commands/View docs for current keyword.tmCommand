	keyword="$TM_SELECTED_TEXT";
else
	keyword="$TM_CURRENT_WORD";
fi

echo "&lt;meta http-equiv=\"refresh\" content=\"0;url=file://$monkey/docs/$preWord/$keyword$postWord\"&gt;";
</string>
	<key>input</key>
	<string>none</string>
	<key>keyEquivalent</key>
	<string>Ôúê</string>
